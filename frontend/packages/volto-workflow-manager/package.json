    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plone/scripts": "^3.6.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "13.13.2",
    "release-it": "^17.1.1"
  }
}
